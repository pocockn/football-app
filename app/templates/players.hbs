<h1>Players</h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Team</th>
    </tr>
  </thead>
  <body>
    {{#each model as |player|}}
      <tr>
        <td>{{player.name}}</td>
        <td>{{player.teams}}</td>
      </tr>
      {{/each}}
  </body>
</table>
